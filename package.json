{
  "name": "@dhruv-techapps/acf-extension",
  "version": "1.0.0",
  "description": "auto click fill extension",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch --env.srcDir=_dev",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --ci --coverage=true --maxWorkers=2",
    "lint": "eslint --cache --format codeframe --ext mjs,js src",
    "lint:fix": "eslint --cache --format codeframe --ext mjs,js src --fix",
    "build:beta:chrome": "webpack --env.srcDir=_beta --env.browser=chrome",
    "build:beta:edge": "webpack --env.srcDir=_beta --env.browser=edge",
    "build:dev:chrome": "webpack --env.srcDir=_dev --env.browser=chrome",
    "build:dev:edge": "webpack --env.srcDir=_dev --env.browser=edge",
    "prepare": "npm run lint:fix && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhruv-Techapps/acf-extension.git"
  },
  "keywords": [
    "auto",
    "click",
    "fill",
    "extension"
  ],
  "author": "Dharmesh",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Dhruv-Techapps/acf-extension/issues"
  },
  "homepage": "https://github.com/Dhruv-Techapps/acf-extension#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "eslint": "^7.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.0",
    "jest-chrome": "^0.7.0",
    "loadash": "^1.0.0",
    "merge-jsons-webpack-plugin": "^1.0.21",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "@dhruv-techapps/acf-common": "^1.0.12",
    "@dhruv-techapps/core-common": "^1.1.59",
    "@dhruv-techapps/core-extension": "^1.0.42"
  }
}
