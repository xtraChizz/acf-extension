{
  "name": "@dhruv-techapps/acf-extension",
  "version": "3.2.14",
  "description": "auto click fill extension",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch --env.srcDir=__local --env.devtool=inline-source-map",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --ci --coverage=true --maxWorkers=2",
    "lint": "eslint --cache --format codeframe --ext mjs,js src",
    "lint:fix": "eslint --cache --format codeframe --ext mjs,js src --fix",
    "build:beta": "webpack --env.srcDir=_beta",
    "build:dev": "webpack --env.srcDir=_dev",
    "build:production": "webpack --env.srcDir=_prod",
    "prepare": "npm run lint:fix && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhruv-Techapps/acf-extension.git"
  },
  "keywords": [
    "auto",
    "click",
    "fill",
    "extension"
  ],
  "author": "Dharmesh",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Dhruv-Techapps/acf-extension/issues"
  },
  "homepage": "https://github.com/Dhruv-Techapps/acf-extension#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "jest": "^26.6.3",
    "jest-chrome": "^0.7.0",
    "loadash": "^1.0.0",
    "merge-jsons-webpack-plugin": "^1.0.21",
    "prettier": "^2.2.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "@dhruv-techapps/acf-common": "^3.1.2",
    "@dhruv-techapps/core-common": "^3.0.0",
    "@dhruv-techapps/core-extension": "^3.0.0"
  }
}
