{
  "name": "@dhruv-techapps/acf-extension",
  "version": "3.2.14",
  "description": "auto click fill extension",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch --mode=development --env goal=__local",
    "lint": "eslint --cache --format stylish --ext mjs,js src",
    "lint:fix": "eslint --cache --format stylish --ext mjs,js src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage=true --maxWorkers=2",
    "build:dev": "webpack --env goal=_dev",
    "build:beta": "webpack --env goal=_beta",
    "build:prod": "webpack --env goal=_prod",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dhruv-Techapps/acf-extension.git"
  },
  "keywords": [
    "auto",
    "click",
    "fill",
    "extension"
  ],
  "author": "Dharmesh",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Dhruv-Techapps/acf-extension/issues"
  },
  "homepage": "https://github.com/Dhruv-Techapps/acf-extension#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.2",
    "jest": "^27.5.1",
    "jest-chrome": "^0.8.0",
    "merge-json-webpack-plugin": "^6.0.0",
    "prettier": "^2.4.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@dhruv-techapps/acf-common": "^5.x",
    "@dhruv-techapps/core-common": "^5.0.8",
    "@dhruv-techapps/core-extension": "^5.0.7",
    "@dhruv-techapps/core-services": "^5.0.8",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0"
  }
